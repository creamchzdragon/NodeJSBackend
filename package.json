    "firebase": "^5.4.2",
    "globalize": "^1.4.0",
    "globalize-webpack-plugin": "^3.0.0",
    "multer": "^1.3.1",
    "react-bootstrap-date-picker": "^5.1.0",
    "react-widgets": "^4.4.4",
    "react-widgets-globalize": "^5.0.13",
    "reactstrap": "^6.4.0"
  }
}
